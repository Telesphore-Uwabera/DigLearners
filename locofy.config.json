{
  "projectName": "DigLearners",
  "framework": "react",
  "bundler": "vite",
  "language": "javascript",
  "styling": "css",
  "units": "px",
  "fileNaming": "pascalCase",
  "components": [
    {
      "name": "Home",
      "path": "frontend/src/pages/public/Home.jsx",
      "type": "page"
    },
    {
      "name": "Login",
      "path": "frontend/src/pages/auth/Login.jsx",
      "type": "page"
    },
    {
      "name": "Enroll",
      "path": "frontend/src/pages/auth/Register.jsx",
      "type": "page"
    },
    {
      "name": "LearnerDashboard",
      "path": "frontend/src/pages/learner/LearnerDashboard.jsx",
      "type": "page"
    },
    {
      "name": "ParentDashboard",
      "path": "frontend/src/pages/parent/ParentDashboard.jsx",
      "type": "page"
    },
    {
      "name": "TeacherDashboard",
      "path": "frontend/src/pages/teacher/TeacherDashboard.jsx",
      "type": "page"
    },
    {
      "name": "AdminDashboard",
      "path": "frontend/src/pages/admin/AdminDashboard.jsx",
      "type": "page"
    },
    {
      "name": "Sidebar",
      "path": "frontend/src/components/layout/Sidebar.jsx",
      "type": "component"
    },
    {
      "name": "AppLayout",
      "path": "frontend/src/components/layout/AppLayout.jsx",
      "type": "component"
    }
  ],
  "styleGuide": {
    "convertFigmaStyles": true,
    "extractCommonStyles": true,
    "appendPrefixToClassnames": false
  },
  "responsive": {
    "breakpoints": {
      "mobile": 480,
      "tablet": 768,
      "desktop": 1024
    }
  },
  "export": {
    "includeTypescript": false,
    "includeStorybook": false,
    "includeTests": false
  }
}